server {
    listen 80;

    access_log /root/MonoReader/monoweb/log/access_log.log
    error_log /root/MonoReader/monoweb/log/error_log.log

    location / {
        proxy_set_header X-Forward-For $proxy_add_x_forwarded_for;
        proxy_set_header Host $http_host;
        proxy_redirect off;
        if (!-f $request_filename) {
            proxy_pass http://127.0.0.1:6000;
        }
    }
}
